<div class="container">
  <div class="row">
    <div class="card-panel"><a href="itinerary/create" class="btn-floating btn-small waves-effect waves-light cyan"><i class="material-icons">add</i></a>
      <p class="parainline">Add your itinerary</p>
    </div>
  </div>
</div>
<div class="container">
  <div class="row">
    <div class="card-panel">
      <p class="center-align">All Your Itinerary<p>
      <% data.forEach(function(element){%>
      <div class="collection">
         <a class="collection-item avatar" href="itinerary/<%= element.id %>">
           <i class="material-icons circle teal">assignment</i>
           <span class="title"><%= element.country %></span>
           <p class="blue-text text-darken-4">$<%= element.budget %>
           </p>
         </a>
         <form action="itinerary/delete/<%= element.id %>" method="POST">
          <button type="submit" class="right-align btn  red darken-4">Delete</button>
         </form>
       </div>
       <% }) %>
    </div>
  </div>
</div>
